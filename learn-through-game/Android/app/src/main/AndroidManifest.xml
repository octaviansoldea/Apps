        <activity
            android:name=".EconFinLessonActivity"
            android:label="@string/title_activity_econfin_lesson"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".EconFinAssignmentActivity"
            android:label="@string/title_activity_econfin_assignment"
